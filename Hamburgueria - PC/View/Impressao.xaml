<Window x:Class="Hamburgueria.View.Impressao"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Hamburgueria.View"
        mc:Ignorable="d"
        Title="IMPRESSÃO" Height="182.957" Width="456.263" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <Grid Background="#FF4472C4">
        <Label Content="IMPRESSORA:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,46,0,0" FontFamily="Arial" FontSize="15" Foreground="White"/>
        <ComboBox x:Name="printsList" VerticalAlignment="Top" Margin="0,46,10,0" HorizontalAlignment="Right" Width="309" FontFamily="Arial" FontSize="15" Height="27"/>
     
        <Button x:Name="cancel" Content="CANCELAR" HorizontalAlignment="Left" VerticalAlignment="Bottom" Width="81" Margin="20,0,0,10" Height="33" Style="{DynamicResource ButtonShadowAround}"/>

        <Button x:Name="print" Content="IMPRIMIR" Margin="0,0,20,10" HorizontalAlignment="Right" Width="82" Height="33" VerticalAlignment="Bottom" Style="{DynamicResource ButtonShadowAround}"/>


    </Grid>
</Window>
